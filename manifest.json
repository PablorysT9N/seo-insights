{
    "manifest_version": 2,
    "name": "SEO Insights",
    "version": "0.0.1",
    "version_name": "0.0.1",
    "description": "SEO Insights is a browser extension to get insights of the current site.",
    "options_page": "options.html",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html",
        "default_title": "SEO Insights"
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}